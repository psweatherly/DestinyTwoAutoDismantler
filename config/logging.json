{
    "formatters":{
        "simple":{
            "format":"%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        }
    },
    "handlers":{
        "D2AutoDismantler":{
            "class":"logging.FileHandler",
            "filename":"logs\\D2AutoDismantler.log",
            "formatter":"simple",
            "level":"DEBUG"
        },
        "console":{
            "class":"logging.StreamHandler",
            "formatter":"simple",
            "level":"DEBUG"
        }
    },
    "loggers":{
        "app":{
            "handlers":[
                "console",
                "D2AutoDismantler"
            ],
            "level":"DEBUG"
        }
    },
    "root":{
        "handlers":[
            "console",
            "D2AutoDismantler"
        ],
        "level":"DEBUG"
    },
    "version":1
}